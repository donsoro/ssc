<div class="stat-block">
  <div class="group-title"><%= statArray[0].type %></div>
  <div class="w-clearfix bar-chart">
    <% _.each(statArray, function(array){ %>
      <div 
        class="bar-chart-element tooltip" 
        title='<%= array.long %>' 
        style='width: <%= array.value %>%; background-color: <%= array.colour %>'
      >
      </div>
    <% }) %>
  </div>
  <div class="block-detail-text">
    R |
    G | 
    SD
  </div>
</div>
