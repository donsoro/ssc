<div class='column medium-12'>
  <a href="#/">< All projects</a>
  <h1><%= project.project_title %></h1>
  <ul>
    <li>
      <% _.each(project.host_location, function(item) { %>
        <%= _.template.partial('filterItem', {item: item, type: 'host_location'}) %>
      <% }) %>

      <% if (project.partner_location.length > 0) { %>
        (collaborated with:
        <% _.each(project.partner_location, function(item) { %>
          <%= _.template.partial('filterItem', {item: item, type: 'partner_location'}) %>
        <% }) %>
        )
      <% } %>
    </li>
    <li><strong>OBJECTIVE:</strong> <%= project.project_objective %></li>
    <li><strong>SSC ACTIVITY:</strong> <%= project.ssc_description %></li>
    <li><strong>PARTNER TYPES:</strong> 
      <% _.each(project.partner_type, function(item) { %>
        <%= _.template.partial('filterItem', {item: item, type: 'partner_type'}) %>
      <% }) %>
    </li>
    <li><strong>THEMES:</strong> 
      <% _.each(project.thematic_focus, function(item) { %>
        <%= _.template.partial('filterItem', {item: item, type: 'thematic_focus'}) %>
      <% }) %>
    </li>
    <li><strong>TERRITORIAL FOCUS:</strong> 
      <% _.each(project.territorial_focus, function(item) { %>
        <%= _.template.partial('filterItem', {item: item, type: 'territorial_focus'}) %>
      <% }) %>
    </li>
  </ul>

  <a href="<%= project.prose_edit_link %>"><i class="fa fa-edit"></i> Edit</a>

  <% if(project.open_project_id != ""){ %>
    <h2>
      Project information from 
      <a href="http://open.undp.org/#project/<%= project.open_project_id %>">open.undp.org</a>
    </h2>
    <iframe id='project_iframe' src="http://open.undp.org/embed.html#widget/project/<%= project.open_project_id %>?stats&documents&outputs" width="100%" height="200px" frameborder="0" marginheight="0" scrolling="no"></iframe>
    <p id='contentTruncated'>
      <a class='triggerIframeResize'><i class='fa fa-expand'></i> Expand</a>
      content from 
      <a href="http://open.undp.org/#project/<%= project.open_project_id %>">open.undp.org</a>
    </p>
  <% } else { %>
    <p>No project data from <a href="http://open.undp.org">open.undp.org</a></p>
  <% } %>
</div>
