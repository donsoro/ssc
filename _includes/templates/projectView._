<a href="#/">< All projects</a>
<h1><%= project.project_title %></h1>
<ul>
  <li>
    <% _.each(project.host_location, function(item) { %>
      <a href="#<%= item.forFilter %>/<%= item.short %>"><%= item.name %></a>
    <% }) %>

    <% if (project.partner_location.length > 0) { %>
      (collaborated with:
      <% _.each(project.partner_location, function(item) { %>
        <a href="#<%= item.forFilter %>/<%= item.short %>"><%= item.name %></a>
      <% }) %>
      )
    <% } %>
  </li>
  <li><%= project.ssc_description %></li>
  <li>
    <% _.each(project.partner_type, function(item) { %>
      <a href="#<%= item.forFilter %>/<%= item.short %>"><%= item.name %></a>
    <% }) %>
  </li>
  <li>
    <% _.each(project.thematic_focus, function(item) { %>
      <a href="#<%= item.forFilter %>/<%= item.short %>"><%= item.name %></a>
    <% }) %>
  </li>
  <li>
    <% _.each(project.territorial_focus, function(item) { %>
      <%= item.name %>
    <% }) %>
  </li>
</ul>

<a href="<%= project.prose_edit_link %>">Edit</a>