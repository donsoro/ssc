<div id='content' class='columns medium-12'>
  <div class='row'>
    <div class='columns medium-12'>
      <ul class="button-group radius">
        <li><a href="#" class="button toggle_list_map">Stats</a></li>
        <li><a href="#" class="button toggle_list_map">Map</a></li>
        <li><a href="#" class="button toggle_list_map">List</a></li>
      </ul>
    </div>
  </div>
  <div class="row">
    <div class='columns medium-12'>
      <p><%= collection.length %> projects</p>
      <a class="scrollControls show-for-small-only"><i class='fa fa-angle-double-up'></i> Jump to FILTERS</a>
      <div id='list'>
        <ul>
          <% _.each(collection, function(project){ %>
            <li>
              <a href="#project/<%= project.project_id %>">
                <%= project.project_title %> [<%= project.project_id %>] 
              </a>
            </li>
          <% }) %>
        </ul>
      </div>
      <div id='map'></div></div>
  </div>
</div>