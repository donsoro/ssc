<div class="w-tabs content-tabs" data-duration-in="50" data-duration-out="50">
  <div class="w-tab-menu tabs-menu">
    <a class="w-tab-link w-inline-block tab-menu-link" data-w-tab="map">
      <div>Map</div>
    </a>
    <a class="w-tab-link w-inline-block tab-menu-link" data-w-tab="stats">
      <div>Stats</div>
    </a>
    <a class="w-tab-link w-inline-block tab-menu-link" data-w-tab="list">
      <div>List</div>
    </a>
  </div>
  <div class="w-tab-content">
    <div class="w-tab-pane tab-content map" data-w-tab="map"><img src="images/Screenshot 2015-02-19 16.07.17.png" alt="54e60bac8fb8905c20c5a907_Screenshot%202015-02-19%2016.07.17.png">
    </div>
    <div class="w-tab-pane tab-content" data-w-tab="stats">
      <div class="w-row">
        <div class="w-col w-col-6">
          <div class="stat-headline">
            <div>26 projects involve more than 2 countries</div>
          </div>
        </div>
        <div class="w-col w-col-6">
          <div class="stat-block">
            <div class="group-title">Thematic focus</div>
            <div class="w-clearfix bar-chart">
              <div class="bar-chart-element element-1"></div>
              <div class="bar-chart-element element-2"></div>
              <div class="bar-chart-element element-3"></div>
            </div>
            <div class="block-detail-text">Resilience |&nbsp;Governance |&nbsp;Sustainable development</div>
          </div>
          <div class="stat-block">
            <div class="group-title">UNDP Role type</div>
            <div class="w-clearfix bar-chart">
              <div class="bar-chart-element element-4"></div>
              <div class="bar-chart-element element-5"></div>
              <div class="bar-chart-element element-6"></div>
            </div>
            <div class="block-detail-text">Capacity |&nbsp;Knowledge |&nbsp;Partnership</div>
          </div>
          <div class="stat-block">
            <div class="group-title">PARTNER&nbsp;TYPE</div>
            <div class="w-clearfix bar-chart">
              <div class="bar-chart-element element-7"></div>
              <div class="bar-chart-element element-8"></div>
              <div class="bar-chart-element element-9"></div>
              <div class="bar-chart-element element-10"></div>
              <div class="bar-chart-element element-11"></div>
            </div>
            <div class="block-detail-text">National govt |&nbsp;International |&nbsp;CSO&nbsp;|&nbsp;Regional |&nbsp;Other</div>
          </div>
        </div>
      </div>
    </div>
    <div class="w-tab-pane tab-content" data-w-tab="list">
      <div class="results-list">
        <% _.each(collection, function(project){ %>
          <a class="w-clearfix w-inline-block results-list-item">
            <div class="list-detail-icon"><img src="images/document blue.svg" width="100%" alt="54ef4c79c09fb76b2c4eb651_document%20blue.svg">
            </div>
            <div class="item-detail">
              <div class="project-title"><%= project.project_title %> [<%= project.project_id %>]</div>
              <div class="project-themes">THEMES: Sustainable development</div>
            </div>
          </a>

        <% }) %>
      </div>
    </div>
  </div>
</div>
