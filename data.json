---
layout: null
---
[{% for project in site.collections.ssc_data.docs %}
    {
        "project_id": "{{ project.relative_path | remove: "_ssc_data/" | remove: ".txt" }}",
        "type": "{{ project.type }}",
        "partners": "{{ project.partners}}",
        "location": "{{ project.location }}",
        "description": "{{ project.content | strip_newlines }}",
        "edit_link": "http://prose.io/#onlyjsmith/jekyll-data/edit/gh-pages/{{ project.relative_path }}"
    }{% unless forloop.last %}, {% endunless %}
{% endfor %}]