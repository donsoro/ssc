        "thematic_focus": "{{ project.thematic_focus }}",
        "type": "{{ project.type }}",
        "location": "{{ project.location }}",
        "partners": "{{ project.partners}}",
        "region": "{{ project.region }}",
        "description": "{{ project.content | strip_newlines }}",
